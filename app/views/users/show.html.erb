<p id="notice"><%= notice %></p>

<h1><%= @user.name %>'s Uploads</h1>

<%= link_to 'Log Out', logout_path, :class=> 'link submit-link logout' %>


<%= form_for @picture, html: { multipart: true } do |f| %>
  <%= f.file_field :uploaded_file %>

  <%= f.submit 'Upload' %>

<% end %>

<table>
  <thead>
    <th> File Name |</th>
    <th> Upload Date </th>
  </thead>
</table>
<% @user.pictures.each do |picture| %>
  <table>
    <tbody>
      <tr> <%= picture.uploaded_file_file_name %>  | </tr>
      <tr> <%= picture.uploaded_file_updated_at %> </tr>
    </tbody>
  </table>
<% end %>



<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
